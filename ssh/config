# sample ssh config
Host tunnel.example.com
  User user_name
  ForwardAgent yes
  Port 2222
Host *.colo
  User user_name2
  ForwardAgent yes
  Port 2222
  ProxyCommand ssh -q tunnel.example.com nc %h %p
Host user_name.tunnel.example.com
  User user_name
  ForwardAgent yes
  Port 2222
Host derp
 User scratch
 Hostname 192.241.227.104
Host fixed-upload 
 User ec2-user 
 Hostname 52.7.122.1
